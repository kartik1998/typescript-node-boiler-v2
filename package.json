  },
  "homepage": "https://github.com/kartik1998/smallurls#readme",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "tsc-watch": "^5.0.3"
  },
  "dependencies": {
    "express": "^4.17.3",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
