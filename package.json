{
  "name": "typescript-boiler",
  "version": "1.0.0",
  "description": "Typescript boiler",
  "main": "dist/index.js",
  "scripts": {
    "compile": "tsc -p tsconfig.json",
    "start": "npm run compile && node dist/index.js",
    "dev": "tsc-watch --project . --outDir ./dist --onSuccess \"node ./dist/index.js\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Kartik",
  "license": "MIT",
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/morgan": "^1.9.3",
    "tsc-watch": "^5.0.3"
  },
  "dependencies": {
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "omnia-logger": "^2.0.2",
    "typescript": "^4.6.3"
  },
  "_moduleAliases": {
    "@lib": "dist/lib",
    "@routes": "dist/routes",
    "@root": "dist",
    "@app": "dist/app"
  }
}
